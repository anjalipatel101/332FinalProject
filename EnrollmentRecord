#Creating the Enrollment Record table 

CREATE TABLE EnrollmentRecord(
  CourseNum INT(6),
  SecNum INT(2),
  CWID VARCHAR(9),
  Grade VARCHAR(2),
  
  FOREIGN KEY (CourseNum,SecNum,CWID);
);



